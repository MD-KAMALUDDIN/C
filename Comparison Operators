#include <stdio.h>

void main() {
    int a=123, b=12;
    
    printf("%d==%d:%d", a, b, a==b);  //0
    printf("\n%d>%d:%d", a, b, a>b);  //1
    printf("\n%d<%d:%d", a, b, a<b);  //0
    printf("\n%d>=%d:%d", a, b, a>=b);  //1
    printf("\n%d<=%d:%d", a, b, a<=b);  //0
    printf("\n%d!=%d:%d", a, b, a!=b);  //1
}
