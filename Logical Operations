#include <stdio.h>

int main() {
    printf("AND Operator\n");
    printf("%d\n", 0&0);
    printf("%d\n", 0&1);
    printf("%d\n", 1&0);
    printf("%d\n", 1&1);
    printf("OR Operator\n");
    printf("%d\n", 0||0);
    printf("%d\n", 0||1);
    printf("%d\n", 1||0);
    printf("%d\n", 1||1);
    printf("NOT Operator\n");
    printf("%d\n", !0);
    printf("%d\n", !1);
    return 0;
}
